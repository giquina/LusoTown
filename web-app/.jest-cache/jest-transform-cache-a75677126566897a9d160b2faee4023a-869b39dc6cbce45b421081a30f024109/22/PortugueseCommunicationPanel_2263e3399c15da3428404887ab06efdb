                                                        })
                                                    })
                                                ]
                                            })
                                        ]
                                    })
                                }, index)) : /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                className: "text-center py-8 text-gray-500",
                                children: [
                                    /*#__PURE__*/ (0, _jsxruntime.jsx)(_lucidereact.Book, {
                                        className: "w-12 h-12 mx-auto mb-3 opacity-50"
                                    }),
                                    /*#__PURE__*/ (0, _jsxruntime.jsx)("p", {
                                        children: t.noResults
                                    })
                                ]
                            })
                        }, "expressions"),
                        activeTab === "reactions" && /*#__PURE__*/ (0, _jsxruntime.jsxs)(_framermotion.motion.div, {
                            initial: {
                                opacity: 0,
                                y: 10
                            },
                            animate: {
                                opacity: 1,
                                y: 0
                            },
                            exit: {
                                opacity: 0,
                                y: -10
                            },
                            className: "space-y-4",
                            children: [
                                /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                    className: "flex space-x-2 mb-4",
                                    children: Object.keys(_portugueseemojis.PORTUGUESE_REACTIONS).map((category)=>/*#__PURE__*/ (0, _jsxruntime.jsx)("button", {
                                            onClick: ()=>setReactionCategory(category),
                                            className: `
                      px-3 py-1 rounded-full text-sm font-medium transition-colors
                      ${reactionCategory === category ? "bg-primary-600 text-white" : "bg-gray-100 text-gray-700 hover:bg-gray-200"}
                    `,
                                            style: reactionCategory === category ? {
                                                backgroundColor: _brand.PORTUGUESE_COLORS.primary
                                            } : {},
                                            children: t[category]
                                        }, category))
                                }),
                                /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                    className: "grid grid-cols-1 sm:grid-cols-2 gap-3",
                                    children: _portugueseemojis.PORTUGUESE_REACTIONS[reactionCategory].map((reaction, index)=>/*#__PURE__*/ (0, _jsxruntime.jsx)(_framermotion.motion.button, {
                                            whileHover: {
                                                scale: 1.02
                                            },
                                            whileTap: {
                                                scale: 0.98
                                            },
                                            onClick: ()=>onSelectExpression(reaction.text, reaction.emoji),
                                            className: "p-3 bg-white border border-gray-200 rounded-lg hover:bg-gray-50 text-left group transition-colors",
                                            children: /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                                className: "flex items-center space-x-3",
                                                children: [
                                                    /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                                        className: "text-2xl",
                                                        children: reaction.emoji
                                                    }),
                                                    /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                                        children: [
                                                            /*#__PURE__*/ (0, _jsxruntime.jsx)("p", {
                                                                className: "font-medium text-gray-900",
                                                                children: reaction.text
                                                            }),
                                                            /*#__PURE__*/ (0, _jsxruntime.jsx)("p", {
                                                                className: "text-sm text-gray-600",
                                                                children: reaction.meaning
                                                            })
                                                        ]
                                                    })
                                                ]
                                            })
                                        }, index))
                                })
                            ]
                        }, "reactions")
                    ]
                })
            })
        ]
    });
