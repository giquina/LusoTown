    const getCelebrationTypeColor = (type)=>{
        switch(type){
            case "festival":
                return "bg-purple-100 text-purple-700 border-purple-200";
            case "independence":
                return "bg-red-100 text-red-700 border-red-200";
            case "cultural":
                return "bg-blue-100 text-blue-700 border-blue-200";
            case "religious":
                return "bg-indigo-100 text-indigo-700 border-indigo-200";
            case "music":
                return "bg-green-100 text-green-700 border-green-200";
            default:
                return "bg-primary-100 text-primary-700 border-primary-200";
        }
    };
    return /*#__PURE__*/ (0, _jsxruntime.jsxs)(_framermotion.motion.div, {
        className: "bg-white rounded-xl shadow-md hover:shadow-xl border border-primary-100 overflow-hidden h-full flex flex-col transition-all duration-300",
        whileHover: {
            y: -2
        },
        children: [
            /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                className: "relative h-32 bg-gradient-to-br from-primary-100 to-gold-50 p-5",
                children: /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                    className: "flex items-start justify-between",
                    children: [
                        /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                            children: [
                                /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                    className: `inline-block text-xs font-semibold px-3 py-1 rounded-full border mb-2 ${getCelebrationTypeColor(celebration.celebrationType)}`,
                                    children: celebration.celebrationType.replace("-", " ").toUpperCase()
                                }),
                                /*#__PURE__*/ (0, _jsxruntime.jsx)("h3", {
                                    className: "text-lg font-bold text-gray-900 line-clamp-2",
                                    children: celebration.title[language]
                                })
                            ]
                        }),
                        /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                            className: "text-3xl",
                            children: celebration.flagEmoji || "\uD83C\uDFAD"
                        })
                    ]
                })
            }),
            /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                className: "p-5 flex-grow flex flex-col",
                children: [
                    /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                        className: "flex items-center mb-3",
                        children: [
                            /*#__PURE__*/ (0, _jsxruntime.jsx)(_outline.ClockIcon, {
                                className: "w-4 h-4 text-primary-500 mr-2"
                            }),
                            /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                className: "text-sm font-medium text-gray-700",
                                children: celebration.period[language]
                            })
                        ]
                    }),
                    celebration.description && /*#__PURE__*/ (0, _jsxruntime.jsx)("p", {
                        className: "text-sm text-gray-600 mb-4 line-clamp-3",
                        children: celebration.description[language]
                    }),
                    celebration.significance && /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                        className: "mb-4 flex-grow",
                        children: [
                            /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                className: "text-xs font-semibold text-gray-800 mb-2",
                                children: language === "en" ? "Cultural Significance:" : "Significado Cultural:"
                            }),
                            /*#__PURE__*/ (0, _jsxruntime.jsx)("p", {
                                className: "text-xs text-gray-600 line-clamp-3",
                                children: celebration.significance[language]
                            })
                        ]
                    }),
                    celebration.traditionalElements && celebration.traditionalElements.length > 0 && /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                        className: "mb-4",
                        children: [
                            /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                className: "text-xs font-semibold text-gray-800 mb-2",
                                children: language === "en" ? "Traditional Elements:" : "Elementos Tradicionais:"
                            }),
                            /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                className: "flex flex-wrap gap-1",
                                children: [
                                    celebration.traditionalElements.slice(0, 4).map((element, index)=>/*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                            className: "text-xs bg-primary-50 text-primary-700 px-2 py-1 rounded-full border border-primary-200",
                                            children: element
                                        }, index)),
                                    celebration.traditionalElements.length > 4 && /*#__PURE__*/ (0, _jsxruntime.jsxs)("span", {
                                        className: "text-xs text-gray-500",
                                        children: [
                                            "+",
                                            celebration.traditionalElements.length - 4
                                        ]
                                    })
                                ]
                            })
                        ]
                    }),
                    celebration.countries && celebration.countries.length > 0 && /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                        className: "mb-4",
                        children: [
                            /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                className: "text-xs font-semibold text-gray-800 mb-2",
                                children: language === "en" ? "Celebrated by:" : "Celebrado por:"
                            }),
                            /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                className: "flex flex-wrap gap-2",
                                children: celebration.countries.map((country, index)=>/*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                        className: "text-xl",
                                        title: country,
                                        children: getCountryFlag(country)
                                    }, index))
                            })
                        ]
                    }),
                    onLearnMoreClick && /*#__PURE__*/ (0, _jsxruntime.jsx)("button", {
                        onClick: ()=>onLearnMoreClick(celebration.id),
                        className: "w-full bg-gradient-to-r from-primary-500 to-primary-600 text-white font-semibold py-3 rounded-lg hover:from-primary-600 hover:to-primary-700 transition-all duration-300 shadow-md hover:shadow-lg",
                        children: language === "en" ? "Learn More" : "Saber Mais"
                    })
                ]
            })
        ]
    });
}

