const getNumericStat = (stat)=>{
    return parseInt(stat.replace(/[^\d]/g, "")) || 0;
};
const formatStat = (stat, showPlus = true)=>{
    const num = getNumericStat(stat);
    return showPlus ? `${num}+` : num.toString();
};

