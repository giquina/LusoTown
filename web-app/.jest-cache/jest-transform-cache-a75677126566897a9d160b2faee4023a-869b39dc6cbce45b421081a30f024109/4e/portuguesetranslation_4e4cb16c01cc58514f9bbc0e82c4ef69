const PORTUGUESE_IDIOMS = {
    "pt-PT": {
        "\xc1gua mole em pedra dura tanto bate at\xe9 que fura": "Persistence pays off",
        "Quem n\xe3o arrisca n\xe3o petisca": "Nothing ventured, nothing gained",
        "Em casa de ferreiro, espeto de pau": "The cobbler's children have no shoes",
