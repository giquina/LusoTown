const shouldLog = ()=>{
    if (typeof window !== "undefined") {
        // @ts-ignore allow runtime flag
        return process.env.NODE_ENV !== "production" || Boolean(window.LUSOTOWN_DEBUG);
    }
    return process.env.NODE_ENV !== "production";
};
const format = (level, args)=>[
        `[LusoTown:${level}]`,
        ...args
    ];
const logger = {
    info: (...args)=>{
        if (!shouldLog()) return;
        console.log(...format("info", args));
    },
    warn: (...args)=>{
        if (!shouldLog()) return;
        console.warn(...format("warn", args));
    },
    error: (...args)=>{
        // Allow errors in all envs, but still route through single point
        console.error(...format("error", args));
    }
};
const _default = logger;

