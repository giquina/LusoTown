{
  "name": "lusotown-mobile",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest __tests__/unit",
    "test:integration": "jest __tests__/integration",
    "test:portuguese": "jest __tests__/portuguese",
    "test:performance": "jest __tests__/performance",
    "test:accessibility": "jest __tests__/accessibility",
    "test:all": "jest --coverage --maxWorkers=2",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src/ --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "performance:analyze": "node scripts/analyze-performance.js",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "build:all": "eas build --platform all",
    "build:preview": "eas build --platform all --profile preview",
    "submit": "eas submit --platform all",
    "update": "eas update",
    "tunnel": "expo start --tunnel",
    "start:clear": "expo start --clear",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "audit:hardcoding": "node scripts/mobile-hardcoding-audit.js",
    "install:ios": "cd ios && pod install",
    "reset:cache": "npm start -- --reset-cache",
    "doctor": "expo doctor"
  },
  "dependencies": {
    "expo": "~52.0.0",
    "react": "18.3.1",
    "react-native": "0.76.1",
    "@react-navigation/native": "^7.0.0",
    "@react-navigation/stack": "^7.0.0",
    "@react-navigation/bottom-tabs": "^7.0.0",
    "react-native-screens": "~4.1.0",
    "react-native-safe-area-context": "~4.12.0",
    "expo-camera": "~16.0.0",
    "expo-image-picker": "~16.0.0",
    "expo-notifications": "~0.29.0",
    "expo-constants": "~17.0.0",
    "expo-font": "~12.0.0",
    "expo-localization": "~16.0.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@supabase/supabase-js": "^2.56.0",
    "formik": "^2.4.0",
    "yup": "^1.4.0",
    "react-native-paper": "^5.12.0",
    "@react-native-community/datetimepicker": "^8.0.0",
    "react-native-url-polyfill": "^2.0.0",
    "i18next": "^23.7.6",
    "react-i18next": "^13.5.0",
    "react-native-reanimated": "~3.16.1",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-svg": "15.8.0",
  "@expo/vector-icons": "^14.0.0",
  "expo-linking": "~6.3.1",
  "expo-location": "~17.0.1",
  "expo-permissions": "~14.4.0",
  "expo-status-bar": "~2.0.0",
  "expo-splash-screen": "~0.29.13",
  "expo-app-loading": "~2.1.1",
  "expo-updates": "~0.26.8",
  "expo-dev-client": "~4.0.20",
    "@lusotown/shared": "file:../shared",
    "date-fns": "^4.1.0",
    "react-native-maps": "1.18.0",
    "react-native-qrcode-svg": "^6.3.0",
    "react-native-keychain": "^8.2.0",
    "react-native-biometrics": "^3.0.1",
    "@react-native-community/netinfo": "11.4.1",
    "react-native-device-info": "^13.0.0",
    "expo-local-authentication": "~14.0.1",
    "expo-secure-store": "~13.0.2",
    "react-native-modal": "^13.0.1",
    "react-native-super-grid": "^6.0.1",
    "react-native-snap-carousel": "^3.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@types/react": "~18.2.45",
    "@types/react-native": "~0.72.6",
    "typescript": "~5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "@testing-library/react-native": "^12.7.2",
    "@testing-library/jest-native": "^5.4.3",
    "react-test-renderer": "18.3.1",
    "@types/jest": "^29.5.0",
    "jest-expo": "~51.0.0",
    "prettier": "^3.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "@expo/cli": "^0.22.2",
    "eas-cli": "^7.8.4",
  "detox": "^20.28.2",
  "@config-plugins/detox": "^8.0.0"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": ["@testing-library/jest-native/extend-expect"],
    "testMatch": [
      "**/__tests__/**/*.test.{js,jsx,ts,tsx}"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/lib/supabase.js"
    ],
    "coverageReporters": ["text", "lcov", "html"],
    "coverageDirectory": "coverage",
    "testTimeout": 10000,
    "moduleNameMapping": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "private": true
}
