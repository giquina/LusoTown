{
  "version": 2,
  "name": "lusotown",
  "builds": [
    {
      "src": "web-app/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "web-app/$1"
    }
  ],
  "env": {
    "NODE_VERSION": "18.x"
  },
  "buildCommand": "cd web-app && npm run build",
  "installCommand": "cd web-app && npm install"
}
